#include <iostream>
using namespace std;

int main() {
    double s, k, d;

    // a)
    s = 14; k = -3;
    d = s + 1;
    s = d;
    k = 2*s;
    cout << s << " " << k << "\n";

    // Ð±)
    s = 0; k = 30;
    d = k - 5;
    k = 2*d;
    s = k - 100;
    cout << s << " " << k << "\n";

    return 0;
}
