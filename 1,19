#include <iostream>
using namespace std;

int main() {
    double k; cin >> k;
    double x;

    // a)
    x = 10; x = -10;
    cout << x << "\n";

    // б)
    x = 17.5; x = -2*x;
    cout << x << "\n";

    // в)
    x = 60; x = x - 1; x = 0;
    cout << x << "\n";

    // г)
    x = -50; x = -25; x = x + k;
    cout << x << "\n";

    return 0;
}
