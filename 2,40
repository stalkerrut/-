#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double y;
    cin >> y; // градусы, 0<=y<360

    double total_minutes = 2.0 * y; 
    long long full_minutes = (long long)floor(total_minutes + 1e-12);

    cout << full_minutes / 60 << "\n";
    cout << full_minutes % 60 << "\n";
    return 0;
}
