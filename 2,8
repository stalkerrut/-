#include <iostream>
using namespace std;

int main() {
    int k, d;
    cin >> k >> d; // k (1..365), d (1..7) для пункта (в)

    int a = (k - 1) % 7 + 1;           
    int b = (k - 1 + 1) % 7 + 1;       
    int c = (k - 1 + (d - 1)) % 7 + 1; 

    auto toN = [](int day){ return day == 7 ? 0 : day; };

    cout << toN(a) << "\n";
    cout << toN(b) << "\n";
    cout << toN(c) << "\n";
    return 0;
}
