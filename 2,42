#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int h, m;
    cin >> h >> m;

    double hour = 30.0 * (h % 12) + 0.5 * m;
    double minute = 6.0 * m;

    auto mod360 = [](double x) {
        x = fmod(x, 360.0);
        if (x < 0) x += 360.0;
        return x;
    };

    double diff = mod360(minute - hour); 
    double speed = 5.5;                 

    // а) совпадут: diff -> 0 (следующее совпадение)
    double ta = (diff == 0.0) ? 0.0 : (360.0 - diff) / speed;

    // б) перпендикулярны: diff -> 90 или 270
    double t90  = mod360(90.0  - diff) / speed;
    double t270 = mod360(270.0 - diff) / speed;
    double tb = min(t90, t270);

    long long ansA = (long long)ceil(ta - 1e-12);
    long long ansB = (long long)ceil(tb - 1e-12);

    cout << ansA << "\n" << ansB << "\n";
    return 0;
}
