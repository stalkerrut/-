#include <iostream>
using namespace std;

int main() {
    int x; cin >> x;
    int a = x / 100, b = (x / 10) % 10, c = x % 10;

    cout << a*100 + b*10 + c << "\n";
    cout << a*100 + c*10 + b << "\n";
    cout << b*100 + a*10 + c << "\n";
    cout << b*100 + c*10 + a << "\n";
    cout << c*100 + a*10 + b << "\n";
    cout << c*100 + b*10 + a << "\n";
    return 0;
}
