#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;

int main() {
    double y;
    cin >> y; // 0<y<=2*pi (радианы)

    const double PI = acos(-1.0);

    double total_minutes = y / (PI / 360.0);     // 360*y/pi
    long long full_minutes = (long long)floor(total_minutes + 1e-12);

    long long full_hours = (full_minutes / 60) % 12;
    long long mins = full_minutes % 60;

    double minute_angle = fmod((fmod(total_minutes, 60.0) + 60.0), 60.0) * (PI / 30.0);

    cout << fixed << setprecision(6) << minute_angle << "\n";
    cout << full_hours << "\n" << mins << "\n";
    return 0;
}
